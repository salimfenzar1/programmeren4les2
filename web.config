<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="Node.js" stopProcessing="true">
                    <match url="/*" />
                    <action type="Rewrite" url="app.js"/>
                </rule>
            </rules>
        </rewrite>
        <staticContent>
            <mimeMap fileExtension=".json" mimeType="application/json" />
            <mimeMap fileExtension=".js" mimeType="text/javascript" />
        </staticContent>
    </system.webServer>
</configuration>